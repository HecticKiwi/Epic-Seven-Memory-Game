{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "linebreak-style": "off",
        "react/jsx-filename-extension": "off",
        "react/prop-types": "off",
        "react/prefer-stateless-function": "off",
        "global-require": "off",
        "import/no-dynamic-require": "off"
    }
}